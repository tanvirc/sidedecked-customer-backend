{
  "name": "@sidedecked/api",
  "version": "1.0.0",
  "description": "SideDecked Customer Backend API Server",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "migration:create": "typeorm migration:create",
    "migration:run": "typeorm migration:run -d src/config/database.ts",
    "migration:revert": "typeorm migration:revert -d src/config/database.ts",
    "migration:show": "typeorm migration:show -d src/config/database.ts",
    "seed": "ts-node src/scripts/seed.ts"
  },
  "dependencies": {
    "@sidedecked/types": "workspace:*",
    "express": "^4.18.2",
    "typeorm": "^0.3.17",
    "pg": "^8.11.3",
    "redis": "^4.6.8",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.5",
    "blurhash": "^2.0.5",
    "minio": "^7.1.3",
    "meilisearch": "^0.34.1",
    "bull": "^4.11.3",
    "node-cron": "^3.0.2",
    "axios": "^1.5.0",
    "winston": "^3.10.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.9",
    "@types/cors": "^2.8.14",
    "@types/compression": "^1.7.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/multer": "^1.4.7",
    "@types/jest": "^29.5.5",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.6.4",
    "ts-jest": "^29.1.1",
    "eslint": "^8.48.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0"
  }
}